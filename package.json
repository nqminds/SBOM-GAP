{
  "name": "@nqminds/vulnerability-analysis-tools",
  "version": "1.0.0",
  "type": "module",
  "description": "Vulnerability tools, provide new tools for identifying SBOM CPE's and list vulnerabilities.",
  "main": "index.mjs",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --forceExit",
    "start-services": "docker-compose up -d",
    "postinstall": "node scripts/download-data.mjs",
    "prettier-check": "prettier --check .",
    "lint": "eslint 'src/**/*.{js,mjs}' 'scripts/**/*.{js,mjs}' 'test/**/*.{js,mjs}' 'index.mjs' --fix"
  },
  "bin": {
    "nqmvul": "index.mjs"
  },
  "author": "nqminds",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^1.3.4",
    "bottleneck": "2.19.5",
    "config": "^3.3.9",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "openai": "^4.19.0",
    "p-queue": "^7.4.1",
    "sax": "^1.3.0",
    "semver": "^7.5.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "xmlbuilder2": "^3.1.1",
    "yauzl": "^2.10.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.1",
    "@jest/globals": "^29.7.0",
    "@nqminds/verifiable-schemas-toolchain": "^1.0.7",
    "docker-compose": "^0.24.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.mjs"
    ],
    "transform": {},
    "testTimeout": 60000
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
